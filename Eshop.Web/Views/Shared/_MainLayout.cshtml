<!DOCTYPE html>
<html lang="fa">
<head>
    <partial name="_MetaTags"/>
    <title>@ViewBag.Title | فروشگاه یکتا کالا</title>
    <partial name="_StyleReferences"/>
</head>
<body>
    <div class="page-wrapper">
        <!-- start of page-header -->
        <partial name="_Header"/>
        <!-- end of page-header -->
        <partial name="_HeaderResponsive" />
        <!-- start of page-content -->
        <main class="page-content">
            @RenderBody()
        </main>
        <!-- end of page-content -->
       <partial name="_Footer"/>
    </div>
    <partial name="_JsReferences"/>
    <partial name="_SiteNotification" />
    @RenderSection("Scripts", false)
</body>
</html>
